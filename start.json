const { exec } = require('child_process');

exec('python chatbot.py', (err, stdout, stderr) => {
  if (err) {
    console.error(`Error executing Python script: ${err}`);
    return;
  }
  if (stderr) {
    console.error(`stderr: ${stderr}`);
    return;
  }
  console.log(`stdout: ${stdout}`);
